{
  "name": "docker-staticmaps",
  "version": "0.1.0",
  "description": "A containerized web version for https://www.npmjs.com/package/staticmaps with https://github.com/expressjs/express",
  "author": "Max Dietrich <mail@mxd.codes>",
  "repository": "github:dietrichmax/docker-staticmaps",
  "scripts": {
    "format": "prettier --write .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit",
