  "type": "module",
  "dependencies": {
    "@turf/bbox": "^6.5.0",
    "@turf/circle": "^6.5.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "got": "^14.1.0",
    "jest": "^29.7.0",
    "lodash.chunk": "^4.2.0",
    "lodash.find": "^4.6.0",
    "lodash.first": "^3.0.0",
    "lodash.flatten": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.last": "^3.0.0",
    "lodash.uniqby": "^4.7.0",
    "modern-async": "^1.1.4",
    "prettier": "^3.2.4",
    "save-dev": "^0.0.1-security",
    "sharp": "^0.33.2",
    "supertest": "^7.0.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
