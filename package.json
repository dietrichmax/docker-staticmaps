{
  "name": "docker-staticmaps",
  "description": "API for generating static map images",
  "author": "Max Dietrich <mail@mxd.codes>",
  "repository": "github:dietrichmax/docker-staticmaps",
  "scripts": {
    "format": "prettier --write .",
    "dev": "nodemon --watch src/** --watch .env --ext ts,js -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./src/server.ts",
    "test": "jest --coverage",
    "build": "webpack",
    "start": "node dist/server.cjs"
  },
  "type": "module",
  "main": "dist/server.cjs",
  "dependencies": {
    "@mapbox/polyline": "^1.2.1",
    "canvas": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.17.2",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/node": "^22.18.1",
    "@types/pdfkit": "^0.14.0",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
